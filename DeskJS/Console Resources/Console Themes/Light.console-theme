	// choosing it from JS Console's options context menu.
	 "icon_package" : "Default Icon Packages/Dark",

	 // this applies the background color of the app...
	 "console_color" : "White",

	 // this applies the outline color (the borders) of the app...
	 "outline_color" : "DarkGray",

	 // this applies the app's title color...
	 "app_title_color" : "Black",

	 // this applies the title labels' color (like dialogs & tooltips)...
	 "title_labels_color" : "Black",

	 // this applies the subtitle labels' color...
	 "subtitle_labels_color" : "Black",

	 // this applies a color for all the textboxes...
	 "textbox_color" : "White",

	 // this applies a fore/font color for all the textboxes...
	 "textbox_fore_color" : "Black",

	 /// this applies the background color for all the buttons;
	 // for beauty and consistency, consider using the outline_color
	 // as the background color of all the buttons...
	 "buttons_color" : "Gray",

	 // this applies text color for all the buttons...
	 "buttons_fore_color" : "White",

	 // this applies the color of the editor's font...
	 "editor_fore_color" : "Black",

	 // refers to the small arrow pointer (>>) right next to the editor...
	 "editor_pointer_color" : "Black",

	 // this applies a color to wrong syntactic statements in JS, for example:
	 // if (1 == 2) { print("True") -- notice that there is no closing bracket.
	 "syntax_errors_color" : "Tomato",

	 // this applies a color to JS errors such as ReferenceErrors, for example:
	 // alerttt("hello world") instead of alert("hello world")...
	 "runtime_errors_color" : "Green",

	 // This group of settings apply to the lists within the app for example,
	 // the themes list or the results panel list

	 // applies the color of the selection in the lists...
	 "list_selection_color" : "DodgerBlue",

	 // applies the color of the default items in the lists...
	 "list_items_color" : "Black",

	 // applies the color of the default subitems in the lists...
	 "list_subitems_color" : "DimGray",

	 // applies the color of the selected items in the lists...
	 "list_selected_items_color" : "White",

	 // applies the color of the selected subitems in the lists...
	 "list_selected_subitems_color" : "White"

